{
  "version": 2,
  "builds": [
    {
      "src": "src/app/api/day5/route.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/day5",
      "dest": "/src/app/api/day5/route.py"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
} 